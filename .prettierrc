{
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 100,
  "pluginSearchDirs": false, 
  "svelteSortOrder": "options-scripts-markup-styles", 
  "svelteStrictMode": false, 
  "svelteAllowShorthand": true, 
  "svelteIndentScriptAndStyle": false,
  "plugins": ["prettier-plugin-svelte"],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
